<style>
    .jumbotron{
    background-image: url("/public/image/banner2.jpg");
    background-repeat: no-repeat;
    background-size: 100%;
    background-color: white;
  }
</style>
<p class="pull-right visible-xs">
        <button type="button" class="btn btn-primary btn-xs" data-toggle="offcanvas" title="Toggle sidebar"><i
                class="fa fa-chevron-right"></i></button>
    </p>

    <div id="carousel-banner" class="carousel slide" data-ride="carousel" data-interval="3500">
            <!--Indicators-->
            <ol class="carousel-indicators">
              <li data-target="#carousel-banner" data-slide-to="0" class="active"></li>
              <li data-target="#carousel-banner" data-slide-to="1"></li>
              <li data-target="#carousel-banner" data-slide-to="2"></li>
              <li data-target="#carousel-banner" data-slide-to="3"></li>
            </ol>
            <!--/.Indicators-->
            <!--Slides-->
            <div class="carousel-inner" role="listbox">
              <div class="item" >
                <img class="d-block w-100" src="public/image/banner3.jpg" alt="First slide">
              </div>
              <div class="item">
                <img class="d-block w-100" src="public/image/banner4.jpg" alt="Second slide">
              </div>
              
              <div class="item">
                <img class="d-block w-100" src="public/image/banner5.jpg" alt="Third slide">
              </div>

              <div class="item active">
                <img class="d-block w-100" src="public/image/banner7.png" alt="Fourth slide">
              </div>
            </div>
            <!--/.Slides-->
            <!--Controls-->
            <a class="carousel-control-prev" href="#carousel-banner" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carousel-banner" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
            <!--/.Controls-->
          </div>
          <!--/.Carousel Wrapper-->        